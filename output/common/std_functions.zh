	bool match;
	for ( int q = 1; q < 32; q++ ) {
		ffc MatchScript = Screen->LoadFFC(q);
		for ( int w = 0; w < SizeOfArray(list); w++ ) {
			if ( MatchScript->Script == script_id ) return q;
		}
	}
	return 0;
}


