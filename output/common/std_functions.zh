//Proximity from centre of object's hitbox:
int ProximityX(ffc a, ffc b, bool fromcentre ) {
	if ( fromcentre ) {
		int ax = CenterX(a);
		int bx = CenterX(b);
		if ( ax > bx ) return ax - bx;
		else return bx - ax;
	}
	else {
		if ( a->X > b->X ) return a->X - b->X;
		else return b->X - a->X;
	}
}

