bool DistXY(ffc a, lweapon b, int distance) {
	int distx; int disty;
	if ( a->X > b->X ) distx = a->X - b->X;
	else distx = b->X - a->X;
	
	if ( a->Y > b->Y ) disty = a->Y - b->Y;
	else disty = b->Y - a->Y;

	return ( distx <= distance && disty <= distance );
} 

